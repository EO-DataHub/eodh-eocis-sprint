{
    "base_dir": "/badc/ukcp18/data",
    "access_control": {
       "rule": "open",
       "roles": []
    },
    "metadata_dir": "/badc/ukcp18/metadata",
    "kerchunk": {
      "dir_depth": {
        "by_dset": 4
      }
    },
    "dset_id_name": "dataset_id",
    "var_id_index_in_dset_id": -3,
    "dset_id_prefix": "ukcp.",
    "defaults": {
      "item": {
        "latest": true
      },
      "asset": {
        "checksum": null,
        "checksum_type": null,
        "type": "application/netcdf",
        "roles": ["data"]
      }
    },
    "global_attrs": ["collection", "domain", "frequency", "institution", 
                     "institution_id", "project", "references", "resolution", "scenario", 
                     "version", "Conventions"],
    "global_attr_map": {},
    "templated_properties": {}
}
